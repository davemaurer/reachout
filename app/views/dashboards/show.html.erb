<h1>Your Dashboard</h1>

<%= form_for @reach do |form| %>
  <%= form.text_field :body, placeholder: "Reach out", required: true %>
  <%= form.submit "Reach out" %>
<% end %>

<% @reaches.each do |reach| %>
  <div>
    <%= reach.user.username %>
    <%= reach.body %>
    <%= time_ago_in_words(reach.created_at) %> ago
  </div>
<% end %>
